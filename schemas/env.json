{
    "environments": [
        {
            "name": "backend",
            "filename": ".env.development",
            "configs": [
                {
                    "name": "DOMAIN",
                    "description": "Domain name for the application",
                    "isRequired": false,
                    "defaultValue": "localhost"
                },
                {
                    "name": "STACK_NAME",
                    "description": "Docker stack name for deployment",
                    "isRequired": false,
                    "defaultValue": "ragverse"
                },
                {
                    "name": "API_V1_STR",
                    "description": "API base path prefix",
                    "isRequired": false,
                    "defaultValue": "/v1"
                },
                {
                    "name": "TRAEFIK_PUBLIC_NETWORK",
                    "description": "Traefik public network name",
                    "isRequired": false,
                    "defaultValue": "ragv_traefik-public"
                },
                {
                    "name": "TRAEFIK_TAG",
                    "description": "Traefik tag for the project",
                    "isRequired": false,
                    "defaultValue": "ragverse"
                },
                {
                    "name": "TRAEFIK_PUBLIC_TAG",
                    "description": "Traefik public tag name",
                    "isRequired": false,
                    "defaultValue": "ragv_traefik-public"
                },
                {
                    "name": "DOCKER_IMAGE_BACKEND",
                    "description": "Docker image name for backend",
                    "isRequired": false,
                    "defaultValue": "ragv_backend"
                },
                {
                    "name": "DOCKER_IMAGE_CELERYWORKER",
                    "description": "Docker image name for Celery worker",
                    "isRequired": false,
                    "defaultValue": "ragv_celeryworker"
                },
                {
                    "name": "DOCKER_IMAGE_FRONTEND",
                    "description": "Docker image name for frontend",
                    "isRequired": false,
                    "defaultValue": "ragv_frontend"
                },
                {
                    "name": "BACKEND_APP_MODULE",
                    "description": "Backend app entry module",
                    "isRequired": false,
                    "defaultValue": "app.main:app"
                },
                {
                    "name": "BACKEND_CORS_ORIGINS",
                    "description": "List of CORS allowed origins",
                    "isRequired": false,
                    "defaultValue": "[\"http://localhost\", \"http://localhost:4200\", \"http://localhost:3000\", \"http://localhost:8080\", \"https://localhost:5173\", \"https://localhost:4200\", \"https://localhost:3000\", \"https://localhost:8080\", \"https://www.ourvedas.in\", \"https://ourvedas.in\", \"http:/frontend\"]"
                },
                {
                    "name": "BACKEND_PRE_START_PATH",
                    "description": "Pre-start script path for backend",
                    "isRequired": false,
                    "defaultValue": "/app/prestart.sh"
                },
                {
                    "name": "PROJECT_NAME",
                    "description": "Name of the project",
                    "isRequired": false,
                    "defaultValue": "RAGVerse"
                },
                {
                    "name": "SECRET_KEY",
                    "description": "Secret key for application security",
                    "isRequired": true,
                    "defaultValue": null
                },
                {
                    "name": "TOTP_SECRET_KEY",
                    "description": "TOTP secret key for 2FA",
                    "isRequired": true,
                    "defaultValue": null
                },
                {
                    "name": "FIRST_SUPERUSER",
                    "description": "Email of the first superuser",
                    "isRequired": true,
                    "defaultValue": null
                },
                {
                    "name": "FIRST_SUPERUSER_PASSWORD",
                    "description": "Password for the first superuser",
                    "isRequired": true,
                    "defaultValue": null
                },
                {
                    "name": "SMTP_TLS",
                    "description": "Enable TLS for SMTP",
                    "isRequired": false,
                    "defaultValue": true
                },
                {
                    "name": "SMTP_PORT",
                    "description": "SMTP server port",
                    "isRequired": false,
                    "defaultValue": 587
                },
                {
                    "name": "SMTP_HOST",
                    "description": "SMTP server hostname",
                    "isRequired": false,
                    "defaultValue": "yoursmtphost"
                },
                {
                    "name": "SMTP_USER",
                    "description": "SMTP server username",
                    "isRequired": false,
                    "defaultValue": "yoursmtpuser"
                },
                {
                    "name": "SMTP_PASSWORD",
                    "description": "SMTP server password",
                    "isRequired": false,
                    "defaultValue": "yoursmtppassword"
                },
                {
                    "name": "EMAILS_FROM_EMAIL",
                    "description": "Default sender email address",
                    "isRequired": false,
                    "defaultValue": "fromemail@domain.com"
                },
                {
                    "name": "EMAILS_FROM_NAME",
                    "description": "Default sender name",
                    "isRequired": false,
                    "defaultValue": "RagVerseTeam"
                },
                {
                    "name": "EMAILS_TO_EMAIL",
                    "description": "Default receiver email address",
                    "isRequired": false,
                    "defaultValue": "fromemail@domain.com"
                },
                {
                    "name": "USERS_OPEN_REGISTRATION",
                    "description": "Allow open registration for users",
                    "isRequired": false,
                    "defaultValue": true
                },
                {
                    "name": "SENTRY_DSN",
                    "description": "Sentry DSN for error monitoring",
                    "isRequired": false,
                    "defaultValue": ""
                },
                {
                    "name": "FLOWER_BASIC_AUTH",
                    "description": "Basic auth credentials for Flower",
                    "isRequired": false,
                    "defaultValue": "admin:superflower"
                },
                {
                    "name": "MONGO_DATABASE_URI",
                    "description": "MongoDB connection URI",
                    "isRequired": false,
                    "defaultValue": "mongodb://yourmongouser:yourmongopassword@mongodb:27017/"
                },
                {
                    "name": "MONGO_DATABASE",
                    "description": "MongoDB database name",
                    "isRequired": false,
                    "defaultValue": "ragverse"
                },
                {
                    "name": "IMAGE_REPO",
                    "description": "Image repository for docker images",
                    "isRequired": false,
                    "defaultValue": "milvusdb"
                },
                {
                    "name": "IMAGE_ARCH",
                    "description": "Image architecture type",
                    "isRequired": false,
                    "defaultValue": "amd64"
                },
                {
                    "name": "OS_NAME",
                    "description": "Operating system name",
                    "isRequired": false,
                    "defaultValue": "ubuntu20.04"
                },
                {
                    "name": "DATE_VERSION",
                    "description": "Build date version for builder",
                    "isRequired": false,
                    "defaultValue": "20240520-d27db99"
                },
                {
                    "name": "LATEST_DATE_VERSION",
                    "description": "Latest build date version for builder",
                    "isRequired": false,
                    "defaultValue": "20240520-d27db99"
                },
                {
                    "name": "GPU_DATE_VERSION",
                    "description": "Build date version for GPU builder",
                    "isRequired": false,
                    "defaultValue": "20240520-c35eaaa"
                },
                {
                    "name": "LATEST_GPU_DATE_VERSION",
                    "description": "Latest build date version for GPU builder",
                    "isRequired": false,
                    "defaultValue": "20240520-c35eaaa"
                },
                {
                    "name": "MINIO_ADDRESS",
                    "description": "MinIO server address",
                    "isRequired": false,
                    "defaultValue": "minio:9000"
                },
                {
                    "name": "PULSAR_ADDRESS",
                    "description": "Pulsar server address",
                    "isRequired": false,
                    "defaultValue": "pulsar://pulsar:6650"
                },
                {
                    "name": "ETCD_ENDPOINTS",
                    "description": "ETCD server endpoint",
                    "isRequired": false,
                    "defaultValue": "etcd:2379"
                },
                {
                    "name": "MILVUS_TOKEN",
                    "description": "Milvus access token",
                    "isRequired": true,
                    "defaultValue": null
                },
                {
                    "name": "MILVUS_URL",
                    "description": "Milvus server URL",
                    "isRequired": false,
                    "defaultValue": "http://ragv_milvus-standalone:19530/"
                },
                {
                    "name": "GROQ_API_KEY",
                    "description": "GROQ API key",
                    "isRequired": false,
                    "defaultValue": "your_groq_api_key"
                },
                {
                    "name": "GROQ_MODEL_NAME",
                    "description": "GROQ model name",
                    "isRequired": false,
                    "defaultValue": "llama-3.2-3b-preview"
                },
                {
                    "name": "LLM_PROVIDER",
                    "description": "LLM provider service",
                    "isRequired": false,
                    "defaultValue": "ollama"
                },
                {
                    "name": "LLM_MODEL_NAME",
                    "description": "LLM model name",
                    "isRequired": false,
                    "defaultValue": "llama3.2:latest"
                },
                {
                    "name": "LLM_URL",
                    "description": "LLM server URL",
                    "isRequired": false,
                    "defaultValue": "http://host.docker.internal:11434"
                },
                {
                    "name": "EMBEDDING_MODEL_NAME",
                    "description": "Embedding model name",
                    "isRequired": false,
                    "defaultValue": "intfloat/e5-base-v2"
                },
                {
                    "name": "EMBEDDING_MODEL_PATH",
                    "description": "Path to the embedding model",
                    "isRequired": false,
                    "defaultValue": "/app/models/e5-base-v2"
                },
                {
                    "name": "REDIS_CHAT_STORE_URL",
                    "description": "Redis server URL for chat storage",
                    "isRequired": false,
                    "defaultValue": "redis://redis:6379"
                },
                {
                    "name": "TRAEFIK_SSLEMAIL",
                    "description": "Email for Traefik SSL certificate",
                    "isRequired": false,
                    "defaultValue": "yourtraefikemail@domain.com"
                },
                {
                    "name": "CERT_RESOLVER",
                    "description": "Certificate resolver for SSL",
                    "isRequired": false,
                    "defaultValue": "letsencrypt"
                },
                {
                    "name": "TRAEFIK_USER",
                    "description": "Traefik dashboard username",
                    "isRequired": false,
                    "defaultValue": "admin_user"
                },
                {
                    "name": "TRAEFIK_PASSWORD_HASH",
                    "description": "Hashed password for Traefik dashboard",
                    "isRequired": false,
                    "defaultValue": "yourtraefikpasswordhash"
                },
                {
                    "name": "SERVER_NAME",
                    "description": "Server name for the backend API",
                    "isRequired": false,
                    "defaultValue": "ragverse-api"
                },
                {
                    "name": "SERVER_HOST",
                    "description": "Host URL for the backend API",
                    "isRequired": false,
                    "defaultValue": "http://localhost:8000"
                }
            ]
        },
        {
            "name": "frontend",
            "filename": "frontend/.env.local",
            "configs": [
                {
                    "name": "DEFAULT_MODEL",
                    "description": "The default model to use in RagVerse.",
                    "isRequired": false,
                    "defaultValue": "ragverse-1.0"
                },
                {
                    "name": "OPENAI_API_KEY",
                    "description": "API key for accessing OpenAI services.",
                    "isRequired": true,
                    "defaultValue": ""
                },
                {
                    "name": "NEXT_PUBLIC_DEFAULT_SYSTEM_PROMPT",
                    "description": "Default system prompt for RagVerse frontend.",
                    "isRequired": false,
                    "defaultValue": "You are RagVerse, an integrated solution for assisting with specialized documents using (RAG). Follow the user's instructions carefully and respond using Markdown formatting."
                },
                {
                    "name": "PLUGINS_INTERNAL",
                    "description": "Internal plugins enabled for RagVerse.",
                    "isRequired": false,
                    "defaultValue": "wikipedia_search,google_search,python_interpreter"
                },
                {
                    "name": "PYTHON_INTERPRETER_BACKEND",
                    "description": "Backend endpoint for Python interpreter plugin.",
                    "isRequired": false,
                    "defaultValue": "http://localhost:8080/api/run"
                },
                {
                    "name": "GOOGLE_API_KEY",
                    "description": "API key for using Google search plugin.",
                    "isRequired": true,
                    "defaultValue": ""
                },
                {
                    "name": "GOOGLE_CSE_ID",
                    "description": "Google Custom Search Engine ID.",
                    "isRequired": true,
                    "defaultValue": ""
                },
                {
                    "name": "MONGODB_URI",
                    "description": "MongoDB URI for local environment.",
                    "isRequired": false,
                    "defaultValue": "mongodb://yourmongouser:yourmongopassword@mongodb:27017/"
                },
                {
                    "name": "MONGODB_DB",
                    "description": "MongoDB database name for local use.",
                    "isRequired": false,
                    "defaultValue": "ragverse"
                },
                {
                    "name": "MONGO_INITDB_ROOT_USERNAME",
                    "description": "MongoDB root username for Docker initialization.",
                    "isRequired": false,
                    "defaultValue": "yourmongouser"
                },
                {
                    "name": "MONGO_INITDB_ROOT_PASSWORD",
                    "description": "MongoDB root password for Docker initialization.",
                    "isRequired": false,
                    "defaultValue": "yourmongopassword"
                },
                {
                    "name": "MONGO_INITDB_DATABASE",
                    "description": "MongoDB database name for Docker initialization.",
                    "isRequired": false,
                    "defaultValue": "ragverse"
                },
                {
                    "name": "NEXTAUTH_ENABLED",
                    "description": "Whether NextAuth authentication is enabled.",
                    "isRequired": false,
                    "defaultValue": "false"
                },
                {
                    "name": "NEXTAUTH_DEBUG",
                    "description": "Enable debug mode for NextAuth.",
                    "isRequired": false,
                    "defaultValue": "true"
                },
                {
                    "name": "NEXTAUTH_EMAIL_PATTERN",
                    "description": "Email pattern allowed for NextAuth login.",
                    "isRequired": false,
                    "defaultValue": ".+@example.com"
                },
                {
                    "name": "NEXTAUTH_SECRET",
                    "description": "Secret key for securing NextAuth sessions.",
                    "isRequired": false,
                    "defaultValue": "yournextauthsecret"
                },
                {
                    "name": "NEXTAUTH_SESSION_MAX_AGE",
                    "description": "Maximum age for NextAuth sessions in seconds.",
                    "isRequired": false,
                    "defaultValue": "86400"
                },
                {
                    "name": "GITHUB_CLIENT_ID",
                    "description": "GitHub OAuth client ID for authentication.",
                    "isRequired": false,
                    "defaultValue": "your_github"
                },
                {
                    "name": "GITHUB_CLIENT_SECRET",
                    "description": "GitHub OAuth client secret for authentication.",
                    "isRequired": false,
                    "defaultValue": "your_github"
                },
                {
                    "name": "GOOGLE_CLIENT_ID",
                    "description": "Google OAuth client ID for authentication.",
                    "isRequired": false,
                    "defaultValue": "your_github"
                },
                {
                    "name": "GOOGLE_CLIENT_SECRET",
                    "description": "Google OAuth client secret for authentication.",
                    "isRequired": false,
                    "defaultValue": "your_github"
                },
                {
                    "name": "COGNITO_CLIENT_ID",
                    "description": "Cognito client ID for authentication.",
                    "isRequired": true,
                    "defaultValue": ""
                },
                {
                    "name": "COGNITO_CLIENT_SECRET",
                    "description": "Cognito client secret for authentication.",
                    "isRequired": true,
                    "defaultValue": ""
                },
                {
                    "name": "COGNITO_ISSUER",
                    "description": "Cognito issuer URL for authentication.",
                    "isRequired": true,
                    "defaultValue": ""
                },
                {
                    "name": "AUDIT_LOG_ENABLED",
                    "description": "Enable audit logging feature.",
                    "isRequired": false,
                    "defaultValue": "false"
                },
                {
                    "name": "PROMPT_SHARING_ENABLED",
                    "description": "Enable sharing prompts between users.",
                    "isRequired": false,
                    "defaultValue": "true"
                },
                {
                    "name": "OPENAI_API_HOST",
                    "description": "Base URL for OpenAI API host.",
                    "isRequired": false,
                    "defaultValue": "http:///backend:8000/v1"
                },
                {
                    "name": "DEBUG_AGENT_LLM_LOGGING",
                    "description": "Enable logging for LLM agent debugging.",
                    "isRequired": false,
                    "defaultValue": "false"
                },
                {
                    "name": "SERVER_PLUGINS",
                    "description": "URL to fetch available server plugins.",
                    "isRequired": false,
                    "defaultValue": "http://backend:8000/v1/vedas_plugins"
                },
                {
                    "name": "DOMAIN_URL",
                    "description": "Base domain URL for frontend access.",
                    "isRequired": false,
                    "defaultValue": "http:///127.0.0.1:3000"
                },
                {
                    "name": "NEXTAUTH_URL",
                    "description": "URL for NextAuth service.",
                    "isRequired": false,
                    "defaultValue": "http:///127.0.0.1:3000"
                },
                {
                    "name": "NEXTAUTH_URL_INTERNAL",
                    "description": "Internal URL for NextAuth service.",
                    "isRequired": false,
                    "defaultValue": "http://127.0.0.1:3000"
                },
                {
                    "name": "NEXT_PUBLIC_API_URL",
                    "description": "Public API URL for frontend access.",
                    "isRequired": false,
                    "defaultValue": "http:///host.docker.internal:8000/v1"
                }
            ]
        },
        {
            "name": "milvus",
            "filename": ".env.milvus",
            "configs": [
                {
                    "name": "MINIO_ACCESS_KEY",
                    "description": "Access key for MinIO server.",
                    "isRequired": false,
                    "defaultValue": "minioadmin"
                },
                {
                    "name": "MINIO_SECRET_KEY",
                    "description": "Secret key for MinIO server.",
                    "isRequired": false,
                    "defaultValue": "minioadmin"
                },
                {
                    "name": "ETCD_ENDPOINTS",
                    "description": "Comma-separated list of etcd server endpoints.",
                    "isRequired": false,
                    "defaultValue": "etcd:2379"
                },
                {
                    "name": "MINIO_ADDRESS",
                    "description": "Address of the MinIO server.",
                    "isRequired": false,
                    "defaultValue": "minio:9000"
                },
                {
                    "name": "ETCD_AUTO_COMPACTION_MODE",
                    "description": "Mode for automatic etcd compaction (e.g., 'revision' or 'period').",
                    "isRequired": false,
                    "defaultValue": "revision"
                },
                {
                    "name": "ETCD_AUTO_COMPACTION_RETENTION",
                    "description": "Retention policy for automatic compaction, in number of revisions or time duration.",
                    "isRequired": false,
                    "defaultValue": "1000"
                },
                {
                    "name": "ETCD_QUOTA_BACKEND_BYTES",
                    "description": "Backend database size quota in bytes for etcd.",
                    "isRequired": false,
                    "defaultValue": "4294967296"
                },
                {
                    "name": "ETCD_SNAPSHOT_COUNT",
                    "description": "Number of committed transactions to trigger a snapshot.",
                    "isRequired": false,
                    "defaultValue": "50000"
                }
            ]
        }
    ]
}